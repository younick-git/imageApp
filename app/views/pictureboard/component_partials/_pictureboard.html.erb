<% total_images.map do |data| %>
  <div class="col-md-3">
    <div class="row">
      <div class="thumbnail">
        <img style="width:100%" src="<%= data.image_url %>" alt="Not Found">
        <div class="caption">
          Posted on <b><i> <%= data.created_at %></i></b>
        </div>
      </div>
    </div>
  </div>
<% end %>
